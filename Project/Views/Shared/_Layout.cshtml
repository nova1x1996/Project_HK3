                    <img src="/img/R-DTH.png" width="200" alt="" style="margin-left:2rem" />
                </a>
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbars-host" aria-controls="navbars-rs-food" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <div class="collapse navbar-collapse" id="navbars-host">
                    <ul class="navbar-nav ml-auto">
                        <li class="nav-item active"><a class="nav-link"  asp-controller="Home" asp-action="Index">Home</a></li>
                       
                        <li class="nav-item"><a class="nav-link" asp-controller="SetUpBox" asp-action="Index" >Set-up Box</a></li>
                        <li class="nav-item"><a class="nav-link" asp-controller="Package" asp-action="Index" >Package </a></li>
                               <li class="nav-item"><a class="nav-link" asp-controller="Movie" asp-action="Index" >Movie Booking</a></li>
                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle" href="#" id="dropdown-a" data-toggle="dropdown">Other </a>
                            <div class="dropdown-menu" aria-labelledby="dropdown-a">
                                <a class="dropdown-item" asp-controller="AboutUs" asp-action="Index">About Us</a>
                                <a class="dropdown-item" asp-controller="Contact" asp-action="Index">Contact</a>
                           
                            </div>
                        </li>
                        <li class="nav-item"><a class="nav-link"  asp-controller="Faq" asp-action="Index">F.A.Q</a></li>
                    </ul>
                    
                    <ul class="nav navbar-nav navbar-right">
                         <li style="margin-right:10px"><a class="hover-btn-new log" href="#" data-toggle="modal" data-target="#recharge"><span>Recharge</span></a></li>
                        @if (!string.IsNullOrEmpty(User.Identity.Name))
                        {
                            <li><a class="hover-btn-new log" href="/UserAuthentication/ChangePassword" ><span>ChangePassword</span></a></li>
                            <li><a class="hover-btn-new log" asp-controller="UserAuthentication" asp-action="Logout"><span>Logout</span></a></li>
                        }else{
                            <li><a class="hover-btn-new log" href="/UserAuthentication/Login" ><span>Customer Login</span></a></li>

                        }
                     
                      
                    </ul>
                </div>
            </div>
        </nav>
    </header>
